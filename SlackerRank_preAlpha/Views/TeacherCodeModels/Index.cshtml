@model IEnumerable<SlackerRank_preAlpha.Models.TeacherCodeModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CodeIntro)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CodeSolution)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CodeExe)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CodePath)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodeIntro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodeSolution)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodeExe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodePath)
            </td>
            <td>
                <a asp-action="Index" asp-controller="Compiler" asp-route-id="@item.Id">Solve</a>
                <a asp-action="Edit" asp-controller="TeacherCodeModels" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-controller="TeacherCodeModels" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-controller="TeacherCodeModels" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
